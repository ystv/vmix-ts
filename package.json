{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "vmix-ts",
  "author": "Marks Polakovs",
  "module": "dist/vmix-ts.esm.js",
  "size-limit": [
    {
      "path": "dist/vmix-ts.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/vmix-ts.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/node": "^20.10.5",
    "husky": "^8.0.3",
    "size-limit": "^11.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/qs": "^6.9.10",
    "@types/uuid": "^9.0.7",
    "fast-xml-parser": "^4.3.2",
    "qs": "^6.11.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "resolutions": {
    "**/typescript": "^5.3.3",
    "**/jest": "^29.7.0",
    "**/ts-jest": "^29.1.1"
  }
